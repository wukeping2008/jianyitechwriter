{
  "name": "jianyi-tech-translator",
  "version": "1.0.0",
  "description": "简仪科技产品手册翻译系统 - 专业的PXI模块仪器、数据采集、测试测量文档翻译平台",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && npm run dev",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "cd backend && npm run build",
    "start": "cd backend && npm start",
    "install:all": "npm install && cd frontend && npm install && cd ../backend && npm install",
    "test": "npm run test:frontend && npm run test:backend",
    "test:frontend": "cd frontend && npm test",
    "test:backend": "cd backend && npm test",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down"
  },
  "keywords": [
    "translation",
    "document-processing",
    "ai-translation",
    "pxi-modules",
    "data-acquisition",
    "test-measurement",
    "technical-documentation",
    "jianyi-tech"
  ],
  "author": "简仪科技开发团队",
  "license": "PROPRIETARY",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jianyi-tech/translator.git"
  },
  "bugs": {
    "url": "https://github.com/jianyi-tech/translator/issues"
  },
  "homepage": "https://github.com/jianyi-tech/translator#readme"
}
